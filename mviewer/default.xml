<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application title="QGIS-Server-Stack" mouseposition="true" measuretools="true" exportpng="true" style="css/themes/wet_asphalt.css" togglealllayersfromtheme="true"/>
    <mapoptions maxzoom="16" projection="EPSG:3857" center="-220750.13768758904,6144925.57790189" zoom="8" projextent="-20037508.342789244, -20037508.342789244, 20037508.342789244, 20037508.342789244" />

    <baselayers style="default"><!-- style="default"||gallery" -->
        <baselayer  type="OSM" id="positron" label="Positron" title="CartoDb" thumbgallery="img/basemap/positron.png"
            url="http://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png" maxzoom="20"
            visible="true" attribution="Map tiles by  &lt;a href='http://cartodb.com/attributions'>CartoDb &lt;/a>, under  &lt;a href='http://creativecommons.org/licenses/by/3.0/'>CC BY 3.0 &lt;/a>" />
        <baselayer  type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap" thumbgallery="img/basemap/osm.png"
            url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png"
            attribution="© MapQuest. Données : les contributeurs d'&lt;a href='http://www.openstreetmap.org/' target='_blank'>OpenStreetMap &lt;/a>,  &lt;a href='http://www.openstreetmap.org/copyright' target='_blank'>ODbL &lt;/a>" visible="false"/>
    </baselayers>

    <themes>
        <theme name="Training Manual Data"  collapsed="true" id="high_tide" icon="fas fa-water">
            <group name="Countries" id="high_countries">
                <layer id="countries" name="countries" visible="false" queryable="false"
                       style="" opacity="1" tiled="true" expanded="true"
                       url="http://localhost:4001/service"/>
            </group>
        </theme>
    </themes>
</config>
